  "keywords": [
    "laravel",
    "nova",
    "nova-package",
    "laravel-package",
    "PHP",
    "posts-crud"
  ],
  "license": "MIT",
  "homepage": "https://github.com/sohbatiha/nova-posts-package",
  "authors": [
